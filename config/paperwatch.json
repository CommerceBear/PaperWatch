{
  "host": "<HOST NAME>",
  "port": "<PORT>",
  "retentionPeriod": 3,
  "sources": [
    {
      "prefix": "API-Gateway-Execution-Logs_",
      "consumer": "APIGatewayLogConsumer"
    },
    {
      "prefix": "/aws/lambda/",
      "consumer": "LambdaLogConsumer"
    }
  ],
  "exclude": [],
  "logLevelExtractor": ".* - (error|warn|info|http|verbose|debug|silly): ",
  "defaultLogLevel": "info"
}
